
{
   "title":"pycharm commands",
   "rules":[
      {
         "description":"override pycharm commands",
         "manipulators":[
            {
               "type":"basic",
               "from":{
                  "modifiers":{
                     "mandatory":[
                        "left_command"
                     ]
                  },
                  "key_code":"spacebar"
               },
               "conditions":[
                  {
                     "type":"frontmost_application_if",
                     "bundle_identifiers":[
                        "com.jetbrains.pycharm"
                     ]
                  }
               ],
               "to":[
                  {
                     "repeat":false,
                     "key_code":"spacebar",
                     "modifiers":[
                        "right_option"
                     ]
                  }
               ]
            }
         ]
      }
   ]
}

